BB=/sbin/busybox;

if [ ! -e /sdcard/Synapse/sharpening ];then
	touch /sdcard/Synapse/sharpening;
fi

if [ -f $1 ];then
	if [[ ! -z $2 ]]; then
		if [ -e /set_sharpe ];then
			echo $2 > $1;
			echo $2 > /sdcard/Synapse/sharpening;
			if [ ! -e /bootC ];then
				input keyevent 26;
				input keyevent 26;
				touch /bootC;
			fi
		fi
	fi

	echo `cat $1`
fi
